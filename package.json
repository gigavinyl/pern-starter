{
  "name": "prk-starter",
  "version": "0.0.1",
  "scripts": {
    "test": "mocha shared/tests/*.spec.js --compilers js:babel-register",
    "start:dev": "cross-env NODE_ENV=development node index.js",
    "start": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "css": "lessc static/styles/app.less --clean-css='--s1 --advanced --compatibility=ie8' static/styles/app.min.css",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run css",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server shared"
  },
  "author": "Logan Saunders <gigavinyl@gmail.com>",
  "dependencies": {
    "babel-core": "^6.4.5",
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "cuid": "^1.3.8",
    "express": "^4.13.4",
    "history": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.10.0",
    "moment": "^2.11.2",
    "objection": "^0.4.0",
    "pg": "^4.5.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.1.5",
    "redux-thunk": "^1.0.3",
    "sanitize-html": "^1.11.3",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-register": "^6.4.3",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "clean-css": "^3.4.9",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^3.0.0",
    "deep-freeze": "0.0.1",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "less": "^2.6.0",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "es6": true,
      "classes": true
    },
    "rules": {
      "max-len": [
        1,
        180,
        4
      ],
      "arrow-body-style": [
        0
      ],
      "consistent-return": 0
    }
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
